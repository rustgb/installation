{
  "arch": "z80",
  "atomic-cas": false,
  "cpu": "gameboy",
  "data-layout": "e-m:z-p:16:8-p2:8:8-p3:16:8-i16:8-i24:8-i32:8-i48:8-i64:8-i96:8-f32:8-f64:8-a:8-n8:16-S8",
  "eh-frame-header": false,
  "exe-suffix": ".gb",
  "is-builtin": true,
  "late-link-args": {
    "gcc": [
      "-lgcc"
    ]
  },
  "linker": "/opt/sdcc/bin/sdldgb",
  "llvm-target": "z80-gameboy",
  "max-atomic-width": 0,
  "pre-link-args": {
    "gcc": [
      "-mmcu=atmega328"
    ]
  },
  "relocation-model": "static",
  "target-c-int-width": "16",
  "target-pointer-width": "16"
}
